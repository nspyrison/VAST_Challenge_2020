@article{macfarlane_analysing_nodate,
	title = {Analysing the Higgs Boson Machine Learning Challenge data using random forests},
	abstract = {The Higgs Boson is an elementary part√icle of mass 125GeV, discovered from experiments performed at the Large Hadron Collider in 2012 at s = 8TeV. The collision includes an intermediate process H → τ +τ −produced with a branching fraction of 6.4\%. The Higgs Boson Machine Learning Challenge held in 2014 saw many participants sharing ideas on how to improve selection methods in the {ATLAS} datasets. Here, we attempt this challenge using Random Forests due to its eﬀectiveness in decision making. Four diﬀerent splitting criteria Gini, Information Gain, One-Sided Extreme and One-Sided Purity were used to model our random forests. Classiﬁcation rate of for the background class was found to be 0.76 ± 0.5, 0.75 ± 0.4, 0.53 ± 0.2, 0.66 ± 0.4 for Gini, Information Gain, One-Sided Extreme and One-Sided Purity respectively. The signal class had a classiﬁcation rate of 0.77 ± 0.3, 0.79 ± 0.4, 0.79 ± 0.4, 0.66 ± 0.4 for Gini, Information Gain, One-Sided Extreme and One-Sided Purity respectively. The variable importance was measured according to the splitting criteria, in which some leading order variables were analysed in a predicted model by producing a histogram of the ’true’ data versus density of the predicted model. The preliminary analysis, although lacking in sample size, provided promising indications that it could be an eﬀective method upon further development.},
	pages = {27},
